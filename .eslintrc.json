{
  "extends": "airbnb-base",
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": false
  },
  "rules": {
    "semi": 2,
    "no-console": 0,
    "no-var": 2,
    "max-len": 0,
    "no-param-reassign": 0,
    "consistent-return": 1,
    "object-shorthand": 0,
    "comma-dangle": 0,
    "prefer-destructuring": 0,
    "no-underscore-dangle": 0,
    "arrow-body-style": 0,
    "no-plusplus": 0,
    "no-unused-vars": 1
  }
}
