{
  "extends": "airbnb-base",
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": false
  },
  "rules": {
    "semi": 2,
    "no-console": 0,
    "no-var": 0,
    "max-len": 0,
    "no-param-reassign": 0,
    "consistent-return": 1,
    "object-shorthand": 0
  }
}
